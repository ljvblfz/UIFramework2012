    <SolidColorBrush x:Key="ComponentArt_Menu_SeparatorItemLineBottomColor" Color="#FFFFFFFF" />

    <SolidColorBrush x:Key="ComponentArt_Menu_SubGroupBorder" Color="#000000" />
    <LinearGradientBrush x:Key="ComponentArt_Menu_SubGroupBackground" StartPoint="0,100" EndPoint="300,100" SpreadMethod="Pad" MappingMode="Absolute">
        <GradientStop Color="#ABABAB" Offset="0"/>
        <GradientStop Color="#DFDFDF" Offset="0.099"/>
        <GradientStop Color="#FFFFFF" Offset="0.0991"/>
    </LinearGradientBrush>

    <SolidColorBrush x:Key="ComponentArt_Menu_ItemForegroundDisabled" Color="#bbbbbb"/>

    <SolidColorBrush x:Key="ComponentArt_Menu_ItemForeground" Color="#FF404040"/>
    <SolidColorBrush x:Key="ComponentArt_Menu_ItemForegroundOver" Color="#FF000000"/>
    <LinearGradientBrush x:Key="ComponentArt_Menu_ItemBackgroundNormal" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#00E3E3E3" Offset="0.51"/>
        <GradientStop Color="#00FEFEFE" Offset="0"/>
        <GradientStop Color="#00F1F1F1" Offset="0.49"/>
        <GradientStop Color="#00F6F6F6" Offset="1"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComponentArt_Menu_ItemBorderNormal" Color="#00262626"/>

    <LinearGradientBrush x:Key="ComponentArt_Menu_ItemBackgroundOver" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFFEFEFE" Offset="0.05"/>
        <GradientStop Color="#FFF0EFEF" Offset="0.499"/>
        <GradientStop Color="#FFC6C6C6" Offset="0.4991"/>        
        <GradientStop Color="#FFF0EFEF" Offset="0.95"/>
    </LinearGradientBrush>
    <SolidColorBrush x:Key="ComponentArt_Menu_ItemBorderOver" Color="#FF000000"/>

    <LinearGradientBrush x:Key="ComponentArt_Menu_ContextMenuFlyLineBrush" StartPoint="0,0" EndPoint="0,1" SpreadMethod="Repeat">
        <GradientStop Color="#01444444" Offset="0" />
        <GradientStop Color="#4f444444" Offset=".43" />
        <GradientStop Color="#cc444444" Offset=".44" />
        <GradientStop Color="#cc444444" Offset=".56" />
        <GradientStop Color="#4f444444" Offset=".57" />
        <GradientStop Color="#01444444" Offset="1" />
