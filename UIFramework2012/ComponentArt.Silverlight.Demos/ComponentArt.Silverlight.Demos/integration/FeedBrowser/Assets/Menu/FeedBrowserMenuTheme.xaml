    <LinearGradientBrush x:Key="ComponentArt_Menu_TopItemBackgroundSelected" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#2D96D0" Offset="0"/>
        <GradientStop Color="#2D96D0" Offset="0.469"/>
        <GradientStop Color="#174B68" Offset="0.470"/>
        <GradientStop Color="#2D98D2" Offset="0.975"/>
    </LinearGradientBrush>
