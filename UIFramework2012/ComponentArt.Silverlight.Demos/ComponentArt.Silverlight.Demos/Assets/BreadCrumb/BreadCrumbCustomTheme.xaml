    <SolidColorBrush x:Key="ComponentArt_BreadCrumb_ItemForeground" Color="#FF808080"/>
    <SolidColorBrush x:Key="ComponentArt_BreadCrumb_ItemForegroundOver" Color="#FF404040"/>
