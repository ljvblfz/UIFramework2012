    <sys:String x:Key="ComponentArt_BreadCrumb_TopGroupPopupMargin">1,0,0,0</sys:String>
    <!-- /BreadCrumbItem Popup ComponentArt_BreadCrumb_AbsorbedButton-->

    <ControlTemplate x:Key="ComponentArt_BreadCrumb_ExpandButton" >
        <StackPanel>
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="Expand" >
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ExpandBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="AbsorbedBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Absorb" >
                        <Storyboard>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="ExpandBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="Collapsed"/>
                            </ObjectAnimationUsingKeyFrames>
                            <ObjectAnimationUsingKeyFrames Storyboard.TargetName="AbsorbedBorder" Storyboard.TargetProperty="Visibility" Duration="0">
                                <DiscreteObjectKeyFrame KeyTime="0" Value="Visible"/>
                            </ObjectAnimationUsingKeyFrames>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            <Border x:Name="ExpandBorder" Padding="4,4,4,0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
                <Path Width="4" Height="7" Fill="#ff000000" Stretch="Fill" Stroke="#00000000" Data="M0,0 L0,8 4,4 z" 
                        HorizontalAlignment="Center" VerticalAlignment="Center"/>
            </Border>
            <Border x:Name="AbsorbedBorder" Padding="2,4,2,0" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" Visibility="Collapsed">
                <StackPanel Orientation="Horizontal">
                    <Path Width="4" Height="8" Fill="#00000000" Stretch="Fill" Stroke="#ff000000" Data="M4,0 L0,4 L4,8" 
                            HorizontalAlignment="Center" VerticalAlignment="Center"/>
                    <Path Width="4" Height="8" Fill="#00000000" Stretch="Fill" Stroke="#ff000000" Data="M4,0 L0,4 L4,8" 
                            HorizontalAlignment="Center" VerticalAlignment="Center"/>
                </StackPanel>
            </Border>
        </StackPanel>
    </ControlTemplate>

    <!-- Scroll -->
    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnNormalItemBorder">#FF808080</Color>
    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnNormalItemInnerBorder">#FF808080</Color>
    <LinearGradientBrush x:Key="ComponentArt_BreadCrumb_ScrollBtnBackground" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFffffff" Offset="0"/>
        <GradientStop Color="#FFffffff" Offset="0.522"/>
        <GradientStop Color="#FFffffff" Offset="0.523"/>
        <GradientStop Color="#FFffffff" Offset="0.975"/>
    </LinearGradientBrush>

    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnOverItemBorder">#FF808080</Color>
    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnOverItemInnerBorder">#FF808080</Color>
    <LinearGradientBrush x:Key="ComponentArt_BreadCrumb_ScrollBtnBackgroundOver" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#7FEAF6FD" Offset="0"/>
        <GradientStop Color="#7FEAF6FD" Offset="0.005"/>
        <GradientStop Color="#7FD7EFFC" Offset="0.52"/>
        <GradientStop Color="#7FBDE6FD" Offset="0.521"/>
        <GradientStop Color="#7FA6D9F4" Offset="0.975"/>
        <GradientStop Color="#7FA6D9F4" Offset="0.99"/>
    </LinearGradientBrush>

    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnPressedItemBorder" >#FF808080</Color>
    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnPressedItemInnerBorder" >#FF808080</Color>
    <LinearGradientBrush x:Key="ComponentArt_BreadCrumb_ScrollBtnBackgroundPressed" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FFEAF6FD" Offset="0"/>
        <GradientStop Color="#FFEAF6FD" Offset="0.005"/>
        <GradientStop Color="#FFD7EFFC" Offset="0.52"/>
        <GradientStop Color="#FFBDE6FD" Offset="0.521"/>
        <GradientStop Color="#FFA6D9F4" Offset="0.975"/>
        <GradientStop Color="#FFA6D9F4" Offset="0.99"/>
    </LinearGradientBrush>

    <ControlTemplate x:Key="ComponentArt_BreadCrumb_ScrollUpButton" >
        <Grid x:Name="RootElement">
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="MouseOver" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnOverItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnOverItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="MouseOverRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="NormalRect" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Normal" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="00:00:00.5" Storyboard.TargetName="NormalRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation Duration="00:00:00.5" Storyboard.TargetName="MouseOverRect" Storyboard.TargetProperty="Opacity" To="0" />
                            <DoubleAnimation Duration="00:00:00.0" Storyboard.TargetName="PressedRect" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.0" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnPressedItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.0" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnPressedItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation AutoReverse="True" Duration="00:00:00.0" Storyboard.TargetName="PressedRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation AutoReverse="True" Duration="00:00:00.0" Storyboard.TargetName="MouseOverRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation Duration="00:00:00.0" Storyboard.TargetName="NormalRect" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Disabled" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Opacity" To=".55"/>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            <Border x:Name="BackgroundBorder" Padding="0" BorderBrush="Transparent" HorizontalAlignment="Stretch" BorderThickness=".25" CornerRadius="0" Margin="0,0,0,0">
                <Border x:Name="InnerBorder" BorderBrush="Transparent" HorizontalAlignment="Stretch" BorderThickness=".25" CornerRadius="0">
                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                        <Rectangle RadiusX="0" RadiusY="0" x:Name="NormalRect" Opacity="1" Fill="{StaticResource ComponentArt_BreadCrumb_ScrollBtnBackground}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <Rectangle RadiusX="0" RadiusY="0" x:Name="MouseOverRect" Opacity="0" Fill="{StaticResource ComponentArt_BreadCrumb_ScrollBtnBackgroundOver}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <Rectangle RadiusX="0" RadiusY="0" x:Name="PressedRect" Opacity="0" Fill="{StaticResource ComponentArt_BreadCrumb_ScrollBtnBackgroundPressed}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <Border HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="0,0,0,4">
                            <Path Width="4" Height="7" Fill="#ff000000" Stretch="Fill" Stroke="#00000000" Data="M0,0 L0,8 4,4 z" 
                                    HorizontalAlignment="Center" VerticalAlignment="Center">
                                <Path.RenderTransform>
                                    <RotateTransform CenterX="4" CenterY="4" Angle="270"></RotateTransform>
                                </Path.RenderTransform>
                            </Path>
                        </Border>
                    </Grid>
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>

    <ControlTemplate x:Key="ComponentArt_BreadCrumb_ScrollDownButton">
        <Grid x:Name="RootElement">
            <vsm:VisualStateManager.VisualStateGroups>
                <vsm:VisualStateGroup x:Name="CommonStates">
                    <vsm:VisualState x:Name="MouseOver" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnOverItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnOverItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="MouseOverRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="NormalRect" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Normal" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.5" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.5" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="00:00:00.5" Storyboard.TargetName="NormalRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation Duration="00:00:00.5" Storyboard.TargetName="MouseOverRect" Storyboard.TargetProperty="Opacity" To="0" />
                            <DoubleAnimation Duration="00:00:00.0" Storyboard.TargetName="PressedRect" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Pressed" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.0" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnPressedItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00.0" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00.0" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnPressedItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation AutoReverse="True" Duration="00:00:00.0" Storyboard.TargetName="PressedRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation AutoReverse="True" Duration="00:00:00.0" Storyboard.TargetName="MouseOverRect" Storyboard.TargetProperty="Opacity" To="1" />
                            <DoubleAnimation Duration="00:00:00.0" Storyboard.TargetName="NormalRect" Storyboard.TargetProperty="Opacity" To="0" />
                        </Storyboard>
                    </vsm:VisualState>
                    <vsm:VisualState x:Name="Disabled" >
                        <Storyboard>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00" Storyboard.TargetName="BackgroundBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <ColorAnimationUsingKeyFrames BeginTime="00:00:00" Duration="00:00:00" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="(Border.BorderBrush).(SolidColorBrush.Color)">
                                <SplineColorKeyFrame KeyTime="00:00:00" Value="{StaticResource ComponentArt_BreadCrumb_ScrollBtnNormalItemInnerBorder}"/>
                            </ColorAnimationUsingKeyFrames>
                            <DoubleAnimation Duration="0" Storyboard.TargetName="InnerBorder" Storyboard.TargetProperty="Opacity" To=".55"/>
                        </Storyboard>
                    </vsm:VisualState>
                </vsm:VisualStateGroup>
            </vsm:VisualStateManager.VisualStateGroups>
            <Border x:Name="BackgroundBorder" Padding="0" BorderBrush="Transparent" HorizontalAlignment="Stretch" BorderThickness=".25" CornerRadius="0" Margin="0,0,0,0">
                <Border x:Name="InnerBorder" BorderBrush="Transparent" HorizontalAlignment="Stretch" BorderThickness=".25" CornerRadius="0">
                    <Grid HorizontalAlignment="Stretch" VerticalAlignment="Stretch" >
                        <Rectangle RadiusX="0" RadiusY="0" x:Name="NormalRect" Opacity="1" Fill="{StaticResource ComponentArt_BreadCrumb_ScrollBtnBackground}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <Rectangle RadiusX="0" RadiusY="0" x:Name="MouseOverRect" Opacity="0" Fill="{StaticResource ComponentArt_BreadCrumb_ScrollBtnBackgroundOver}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <Rectangle RadiusX="0" RadiusY="0" x:Name="PressedRect" Opacity="0" Fill="{StaticResource ComponentArt_BreadCrumb_ScrollBtnBackgroundPressed}" HorizontalAlignment="Stretch" VerticalAlignment="Stretch" />
                        <Border HorizontalAlignment="Stretch" VerticalAlignment="Stretch" Padding="0,4,0,0">
                            <Path Width="4" Height="7" Fill="#ff000000" Stretch="Fill" Stroke="#00000000" Data="M0,0 L0,8 4,4 z" 
                                  HorizontalAlignment="Center" VerticalAlignment="Center">
                                <Path.RenderTransform>
                                    <RotateTransform CenterX="4" CenterY="4" Angle="90"></RotateTransform>
                                </Path.RenderTransform>
                            </Path>
                        </Border>
                    </Grid>
                </Border>
            </Border>
        </Grid>
    </ControlTemplate>
