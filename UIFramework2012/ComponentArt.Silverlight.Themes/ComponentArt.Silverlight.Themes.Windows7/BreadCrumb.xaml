    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnPressedItemBorder" >#165993</Color>
    <Color x:Key="ComponentArt_BreadCrumb_ScrollBtnPressedItemInnerBorder" >#165993</Color>
    <LinearGradientBrush x:Key="ComponentArt_BreadCrumb_ScrollBtnBackgroundPressed" EndPoint="0.5,1" StartPoint="0.5,0">
        <GradientStop Color="#FAFCFE" Offset="0" />
        <GradientStop Color="#CFEDFD" Offset="0.1" />
        <GradientStop Color="#6DC1F4" Offset="0.9" />
        <GradientStop Color="#CACFD4" Offset="1" />
